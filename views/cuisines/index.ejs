<%- include('../partials/header') %>

<span>
  <h1><%= title %></h1>
</span>

<div class="section">
  <div class="row">
    <div class="col s12 m12">
      <% if (cuisines.length) { %>
        <% cuisines.forEach(function(c) { %>
      <div class="col s4 m4">
        <div class="card">
          <a href="/cuisines/<%= c._id %>">
            <div class="card-image">
              <img class="cuisine-image" src="<%= c.imageUrl %>">
            </div>
          </a>
          <div class="card-action">
            <div class="row content-box">
              <p class="cuisine-title"><%= c.name %></p>
            </div>
          </div>
        </div>
      </div>
  <% }); %>
  </div>
</div>
  <% } else { %>
  <h3>Please Add cuisines</h3>
  <% } %>


<%- include('../partials/footer') %>